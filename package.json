{
  "name": "@memori.ai/avatar-creator",
  "author": "Memori Srl",
  "version": "0.1.1",
  "main": "dist/memori-ai-avatar-creator.umd.js",
  "module": "dist/memori-ai-avatar-creator.es.js",
  "types": "dist/index.d.ts",
