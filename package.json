  "exports": {
    ".": {
      "import": "./dist/memori-ai-avatar-creator.es.js",
      "require": "./dist/memori-ai-avatar-creator.umd.js"
    },
    "./styles.css": "./src/styles.css"
  },
  "files": [
    "src",
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/memori-ai/avatar-creator#readme",
  "bugs": "https://github.com/memori-ai/avatar-creator/issues",
  "repository": "https://github.com/memori-ai/avatar-creator.git",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
